grammar org.xtext.istic.mapUI.MapUI with org.eclipse.xtext.common.Terminals

generate mapUI "http://www.xtext.org/istic/mapUI/MapUI"

MapUI:
	Mappings+=Mapping*;
	
Mapping:
	name=ID ':' type=Type;

Type: CheckBox | RadioButton | Image | Son;

Image:
	'ImageCheck' | "ImageRadio";

CheckBox:
	'CheckBox';

RadioButton:
	'RadioButton';
	
Son :
	'SonCheck' | "SonRadio";
